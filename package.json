{
	"name": "chadort",
	"license": "(ISC OR GPL-3.0)",
	"scripts": {
		"site:local": "NODE_ENV=local webpack --config .webpack/config.dev.js",
		"site:staging": "NODE_ENV=staging webpack --config .webpack/config.prod.js",
		"site:prod": "NODE_ENV=production webpack --config .webpack/config.prod.js",
		"stack:local": "NODE_ENV=local nodemon --exec babel-node --config-file ./babel.config.server.js stack/local/",
		"stack:local:debug": "NODE_ENV=local nodemon --inspect --exec babel-node --config-file ./babel.config.server.js stack/local/",
		"stack:staging": "NODE_ENV=staging pulumi up --stack chadort",
		"stack:prod": "NODE_ENV=production pulumi up --stack chadort-prod --skip-preview",
		"deploy:staging": "yarn site:staging && yarn stack:staging",
		"deploy:prod": "yarn test && yarn site:prod && yarn stack:prod",
		"lint": "yarn lint:styles && yarn lint:scripts",
		"lint:styles": "stylelint '**/*.scss'",
		"lint:scripts": "eslint .",
		"test": "jest"
	},
